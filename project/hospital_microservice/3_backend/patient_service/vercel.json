
{
	"version": 2,
	"builds": [
		{
			"src": "index.py",
			"use": "@vercel/python"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/"
		}
	],
	"env": {
		"DATABASE_URL": "@DATABASE_URL",
		"TWILIO_ACCOUNT_SID": "@TWILIO_ACCOUNT_SID",
		"TWILIO_AUTH_TOKEN": "@TWILIO_AUTH_TOKEN",
		"SMS_FROM_NUMBER": "@SMS_FROM_NUMBER",
		"SECRET_KEY": "@SECRET_KEY"
	}
}
